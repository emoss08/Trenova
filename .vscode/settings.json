{
    "css.customData": [
        ".vscode/tailwind.json"
    ],
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "punctuation",
                "settings": {
                    "foreground": "#ffffff"
                }
            }
        ]
    },
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#b6b6b6",
        "editorBracketHighlight.foreground2": "#b6b6b6",
        "editorBracketHighlight.foreground3": "#b6b6b6",
        "editorBracketHighlight.unexpectedBracket.foreground": "#afafaf",
    },
    "editor.fontFamily": "CaskaydiaCove Nerd Font",
    "diffEditor.codeLens": true,
    "tooltitude.codeAction.runDebug": false,
    "tooltitude.codeLens.packImports": false,
    "tooltitude.debug.inlineValuesEnabled": false,
    "tooltitude.postfixCompletionEnabled": false,
    "tooltitude.refHighlightsEnabled": false,
    "tooltitude.unhandledErrorsEnabled": true,
    "tooltitude.unreachableStmtsEnabled": true,
    "tooltitude.unusedImportsEnabled": true,
    "tooltitude.unusedWritesEnabled": false,
    "tooltitude.updateImportsOnRename": false,
    "tooltitude.codeAction.navigate": false,
    "glassit.alpha": 250,
    "editor.fontSize": 15
}